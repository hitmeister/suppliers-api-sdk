{
    "name": "hitmeister/suppliers-api-sdk",
    "version": "0.13.0",
    "description": "Suppliers API SDK",
    "keywords": [
        "real.digital",
        "sms.real",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://sms.kaufland.de/api/v1/specs/?url=/api/v2/specs/supplier_api.yaml",
    "license": "MIT",
    "authors": [
        {
            "email": "sms.tech@kaufland-online.com",
            "name": "Kaufland",
            "homepage": "https://github.com/hitmeister/suppliers-api-sdk"
        },
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
